{
  "type": "object",
  "comment": "",
  "properties": {
    "categorylist": {
      "type": "select",
      "selector": "select[name=\"project_id\"]",
      "control": "select"
    },
    "description": {
      "type": "string",
      "selector": "[name=\"description\"]",
      "control": "property.innerText"
    },
    "isreportissue": {
      "type": "identifier",
      "selector": ".form-title",
      "control": "exists",
      "selectContains": "Enter Report Details"
    },
    "summary": {
      "type": "string",
      "selector": "[name=\"summary\"]",
      "control": "property.innerText"
    },
    "submit": {
      "type": "action",
      "selector": "input.button",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isreportissue"
      ],
      "screens": [
        "reportissue"
      ]
    }
  ]
}
